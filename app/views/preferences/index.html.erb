<p id="notice"><%= notice %></p>

<h1>Preferences</h1>

<table>
  <thead>
    <tr>
      <th>Student</th>
      <th>Course</th>
      <th>Subject matter proficiency</th>
      <th>Time zone</th>
      <th>Dream partner</th>
      <th>Schedule</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @preferences.each do |preference| %>
      <tr>
        <td><%= preference.student_id %></td>
        <td><%= preference.course_id %></td>
        <td><%= preference.subject_matter_proficiency %></td>
        <td><%= preference.time_zone %></td>
        <td><%= preference.dream_partner %></td>
        <td><%= preference.schedule %></td>
        <td><%= link_to 'Show', preference %></td>
        <td><%= link_to 'Edit', edit_preference_path(preference) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Preference', new_preference_path %>
